        "    \n",
        "    for p_data in prompts:\n",
        "        print(f\"Testing: {p_data['name']}...\")\n",
        "        full_prompt = f\"<|im_start|>system\\nYou are an expert Reservoir Engineer.<|im_end|>\\n<|im_start|>user\\n{p_data['content']}<|im_end|>\\n<|im_start|>assistant\\n\"\n",
